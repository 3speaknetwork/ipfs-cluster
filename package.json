{
  "name": "@nftstorage/ipfs-cluster",
  "version": "3.3.1",
  "description": "A zero-dependency client to the IPFS Cluster HTTP API, built for the browser.",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js"
  },
  "scripts": {
    "test": "mocha test/*.spec.js",
    "start": "docker-compose up",
    "format": "prettier --write ."
  },
  "author": "Alan Shaw",
  "license": "(Apache-2.0 AND MIT)",
  "devDependencies": {
    "@ipld/car": "^1.0.1",
    "@ipld/dag-cbor": "^5.0.0",
    "@web-std/fetch": "^2.0.0",
    "@web-std/file": "^1.1.0",
    "@web-std/form-data": "^2.1.0",
    "multiformats": "^8.0.6",
    "mocha": "^9.1.3",
    "uvu": "^0.5.2",
    "lint-staged": "^11.0.0",
    "prettier": "2.4.0",
    "simple-git-hooks": "^2.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.*": "prettier --write ."
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": "prettier-standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nftstorage/ipfs-cluster.git"
  },
  "keywords": [
    "ipfs",
    "cluster",
    "client",
    "http",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/nftstorage/ipfs-cluster/issues"
  },
  "homepage": "https://github.com/nftstorage/ipfs-cluster#readme"
}
